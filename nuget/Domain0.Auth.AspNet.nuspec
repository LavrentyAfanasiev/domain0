<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Domain0.Auth.AspNet</id>
    <title>JWT Auth checker for AspNetCore services</title>
    <version>1.0.1</version>
    <authors>Totopolis</authors>
    <owners>Totopolis</owners>
    <summary>JWT Auth checker </summary>
    <description>JWT Auth checker for AspNetCore services</description>
    <projectUrl>https://github.com/Totopolis/domain0</projectUrl>
    <licenseUrl>https://github.com/Totopolis/domain0/blob/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>Domain0 JWT Authentication checker service AspNetCore</tags>
    <language>en-US</language>
    <dependencies>
      <group targetFramework="net461">
        <dependency id="Microsoft.AspNetCore.Authentication.JwtBearer" version="1.1.3"/>
        <dependency id="Microsoft.AspNetCore.Authorization" version="1.1.3"/>
        <dependency id="Microsoft.IdentityModel.JsonWebTokens" version="5.3.0"/>
        <dependency id="Newtonsoft.Json" version="11.0.1"/>
      </group>
      <group targetFramework=".netstandard1.4">
        <dependency id="Microsoft.AspNetCore.Authentication.JwtBearer" version="1.1.3"/>
        <dependency id="Microsoft.AspNetCore.Authorization" version="1.1.3"/>
        <dependency id="Microsoft.IdentityModel.JsonWebTokens" version="5.3.0" />
        <dependency id="Newtonsoft.Json" version="11.0.1"/>
      </group>
      <group targetFramework=".netstandard2.0">
        <dependency id="Microsoft.AspNetCore.Authentication.JwtBearer" version="1.1.3"/>
        <dependency id="Microsoft.AspNetCore.Authorization" version="1.1.3"/>
        <dependency id="Microsoft.IdentityModel.JsonWebTokens" version="5.3.0" />
        <dependency id="Newtonsoft.Json" version="11.0.1"/>
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\prj\Domain0.Auth.AspNet\bin\Release\**\*.*" target="lib" />
    <file src="..\src\token\TokenValidationSettings.cs" target="src" />
    <file src="..\src\Auth.AspNet\AspNetExtensions.cs" target="src" />
  </files>
</package>