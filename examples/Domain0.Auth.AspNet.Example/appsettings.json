{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "TokenValidation": {
    "Audience": "<audience>",
    "Issuer": "<issuer>",
    "ValidateLifetime": true,
    "Keys": [{
      "Alg": "<algorithm>",
      "Key": "<secure-key>"
    }]
  }
}